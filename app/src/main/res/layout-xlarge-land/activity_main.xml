<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

	<ImageView
			android:layout_width="0dp"
			android:layout_height="200dp" app:srcCompat="@drawable/devslopesbanner"
			android:id="@+id/imageView9"
			app:layout_constraintStart_toStartOf="parent"
			app:layout_constraintTop_toTopOf="parent"
			app:layout_constraintEnd_toEndOf="parent" android:scaleType="centerCrop"
			app:layout_constraintHorizontal_bias="0.0"/>
	<ImageView
			android:layout_width="150dp"
			android:layout_height="150dp" app:srcCompat="@drawable/devslopesprofilelogo"
			android:id="@+id/logo"
			app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="32dp"
			android:layout_marginTop="128dp" app:layout_constraintTop_toTopOf="parent" android:scaleType="centerCrop"/>
	<TextView
			android:text="Devslopes"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:id="@+id/textView2" app:layout_constraintStart_toEndOf="@+id/logo"
			android:layout_marginStart="24dp" android:layout_marginTop="24dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView9" android:textSize="24sp"/>
	<ImageView
			android:layout_width="50dp"
			android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
			android:id="@+id/imageView19"
			app:layout_constraintStart_toEndOf="@+id/textView2" app:layout_constraintHorizontal_bias="0.8"
			app:layout_constraintEnd_toStartOf="@+id/imageView20"
			app:layout_constraintHorizontal_chainStyle="packed" android:layout_marginTop="24dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView9"/>
	<ImageView
			android:layout_width="50dp"
			android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
			android:id="@+id/imageView20"
			app:layout_constraintStart_toEndOf="@+id/imageView19"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/imageView21"
			android:layout_marginStart="24dp" app:layout_constraintTop_toTopOf="@+id/imageView19"
			app:layout_constraintBottom_toBottomOf="@+id/imageView19"/>
	<ImageView
			android:layout_width="50dp"
			android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
			android:id="@+id/imageView21"
			app:layout_constraintStart_toEndOf="@+id/imageView20"
			app:layout_constraintHorizontal_bias="0.5" app:layout_constraintEnd_toStartOf="@+id/button2"
			android:layout_marginEnd="32dp" android:layout_marginStart="24dp"
			app:layout_constraintTop_toTopOf="@+id/imageView20"
			app:layout_constraintBottom_toBottomOf="@+id/imageView20"/>
	<Button
			android:text="customize dashboard"
			android:layout_width="250dp"
			android:layout_height="wrap_content"
			android:id="@+id/button2"
			style="@style/Widget.AppCompat.Button.Borderless" app:layout_constraintEnd_toEndOf="parent"
			android:layout_marginEnd="32dp" android:layout_marginTop="24dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView9" android:background="@drawable/rounded_button"
			android:textColor="@android:color/white"/>
	<android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
	                                      android:id="@+id/guideline3"
	                                      android:orientation="vertical" app:layout_constraintGuide_percent="0.5"/>
	<ImageView
			android:layout_width="0dp"
			android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
			android:id="@+id/imageView22" android:layout_marginTop="32dp"
			app:layout_constraintTop_toBottomOf="@+id/logo" app:layout_constraintEnd_toStartOf="@+id/guideline3"
			android:layout_marginEnd="16dp" app:layout_constraintDimensionRatio="16:9"/>
	<ImageView
			android:layout_width="0dp"
			android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
			android:id="@+id/imageView23"
			app:layout_constraintStart_toStartOf="@+id/guideline3" android:layout_marginStart="16dp"
			app:layout_constraintTop_toTopOf="@+id/imageView22" app:layout_constraintDimensionRatio="16:9"/>
	<ImageView
			android:layout_width="0dp"
			android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
			android:id="@+id/imageView24" android:layout_marginTop="32dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView22" app:layout_constraintEnd_toStartOf="@+id/guideline3"
			android:layout_marginEnd="16dp" app:layout_constraintStart_toStartOf="@+id/imageView22"
			android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintDimensionRatio="16:9"/>
	<ImageView
			android:layout_width="0dp"
			android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
			android:id="@+id/imageView25" android:layout_marginTop="32dp"
			app:layout_constraintTop_toBottomOf="@+id/imageView23"
			app:layout_constraintStart_toStartOf="@+id/guideline3" android:layout_marginStart="16dp"
			android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"
			app:layout_constraintEnd_toEndOf="@+id/imageView23" app:layout_constraintDimensionRatio="16:9"/>
</android.support.constraint.ConstraintLayout>